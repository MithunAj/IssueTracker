<body>
    <h4>List of projects</h4>
    <% if(projects){%>
        <div>
            <% for (project of projects) { %>
                <div>
                    <span><%=project.name%> <button><a href="/projects/details/<%=project._id%>">Details</a></button> </span>
                </div>
            <%}%>    
        </div>
    <%} else { %>
        <div>
            <h5>No projects to show</h5>
        </div>
    <%}%>
    
    <button><a href="/projects/new">Create project</a></button>

</body>